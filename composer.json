{
    "name": "symfony-cmf/column-browser-bundle",
    "description": "Resource browser that uses columns.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "@stable",
        "symfony-cmf/resource-bundle": "^1.0@dev"
    },
    "require-dev": {
        "symfony/console": "@stable",
        "symfony/yaml": "@stable",
        "symfony/process": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ColumnBrowserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ColumnBrowserBundle\\Tests\\": "tests/",
            "Symfony\\Cmf\\Bundle\\ColumnBrowserBundle\\Example\\": "example/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
